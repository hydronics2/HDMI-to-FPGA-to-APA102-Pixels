            rgb7Counter.d = rowStartPixel7;  //bottom right
            rgb8Counter.d = rowStartPixel2;  //bottom left
          }     
        }
        
        if(pixelCounter.q == 2039) //all 3 panels captured in ram
        {
          startWritingFlag2 = 1;
          frame2Write.d = 1;
