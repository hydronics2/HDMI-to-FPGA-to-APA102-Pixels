            columnCounter.d = 0;
            rgb3Counter.d = rowStartPixel3;
            rgb4Counter.d = rowStartPixel4;
            rgb5Counter.d = rowStartPixel5;
          }     
        }
        
        if(pixelCounter.q == 2039) //all 3 panels captured in ram
        {
