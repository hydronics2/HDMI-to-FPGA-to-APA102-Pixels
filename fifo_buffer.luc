    simple_ram rgb0 (#SIZE(24), #DEPTH(691));
    simple_ram rgb1 (#SIZE(24), #DEPTH(691));
    simple_ram rgb2 (#SIZE(24), #DEPTH(691));
    
    simple_ram rgb3 (#SIZE(24), #DEPTH(691));
    simple_ram rgb4 (#SIZE(24), #DEPTH(691));
    simple_ram rgb5 (#SIZE(24), #DEPTH(691));
    
