          rgb7Counter.d = writePixelAddress7;
          rgbOut7 = rgb7.read_data;
          
          rgb8.address = writePixelAddress8;
          rgb8Counter.d = writePixelAddress8;
          rgbOut8 = rgb8.read_data;        
        }
      }
    

        
        //-----------------------------FRAME 3 ------- panels 9,10&11 
      if(frameCounter.q == 3)
      {
        if (!fifo.empty && newPixelFlag.q == 0) 
        {
          fifo.rget = 1;
          pixel.d = fifo.dout[23:0];
          newPixelFlag.d = 1;
        }
       
        if(newPixelFlag.q == 1)
        {
          newPixelFlag.d = 2;
          
          if(columnCounter.q < 34)
          {
